    />
    <xacro:monocular_camera_macro
      name="camera8/camera"
      parent="sensor_kit_base_link"
      namespace=""
      x="${calibration['sensor_kit_base_link']['camera8/camera_link']['x']}"
      y="${calibration['sensor_kit_base_link']['camera8/camera_link']['y']}"
      z="${calibration['sensor_kit_base_link']['camera8/camera_link']['z']}"
      roll="${calibration['sensor_kit_base_link']['camera8/camera_link']['roll']}"
      pitch="${calibration['sensor_kit_base_link']['camera8/camera_link']['pitch']}"
      yaw="${calibration['sensor_kit_base_link']['camera8/camera_link']['yaw']}"
      fps="30"
