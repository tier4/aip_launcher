    />
    <xacro:monocular_camera_macro
      name="camera9/camera"
      parent="sensor_kit_base_link"
      namespace=""
      x="${calibration['sensor_kit_base_link']['camera9/camera_link']['x']}"
      y="${calibration['sensor_kit_base_link']['camera9/camera_link']['y']}"
      z="${calibration['sensor_kit_base_link']['camera9/camera_link']['z']}"
